create database avaliacao_22b;
use avaliacao_22b;

create table livro_s (
id_livro  int AUTO_INCREMENT primary key,
titulo  varchar(200),
ano_publicação int,
autor varchar(200),
idioma varchar(100),
disponivel boolean,
isbn varchar(200),
editora varchar(200),
quant_paginas int
);

insert into livro_s( Titulo, Autor, Publicação, Categoria, ISBN, Editora ,Idioma,  quantidade_paginas) value (
   ("As crônicas de Nárnia", "C.S. Lewis", "1950", "Fantasia", "978-0064471190", "HarperCollins", "768")
);
 
select disponivel
from livros 
where ano_publicação > 1900 = false



